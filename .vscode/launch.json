{
    "version": "0.2.0",
    "configurations": [
  
      {
        "name": "Listen for Xdebug",
        "type": "php",
        "request": "launch",
        "port": 9001, 
        "pathMappings": {
          "/var/www/html": "${workspaceFolder}"
        },
        "xdebugSettings": {
          "max_data": 65535,
          "max_children": 100,
          "max_depth": 5
        }
      }
  
    ]
  }